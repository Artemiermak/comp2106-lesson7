<% include ../partials/header.ejs %>

<main class="container">

  <h1>Developer Details</h1>

  <form method="post">
    <div class="form-group">
      <label for="name" class="col-sm-2 control-label">Name: *</label>
      <input name="name" id="name" required />
    </div>
    <div class="form-group">
      <label for="year" class="col-sm-2 control-label">Year: *</label>
      <input type="date" name="year" id="year" required />
    </div>
    <div class="form-group">
      <label for="address" class="col-sm-2 control-label">Address: *</label>
      <input name="address" id="address" required />
    </div>
    <div class="form-group">
      <label for="revenue" class="col-sm-2 control-label">Revenue:</label>
      <input name="revenue" id="revenue" type="number" required />
    </div>
    <button class="btn btn-success col-sm-offset-2">Save</button>
  </form>

</main>

<% include ../partials/footer.ejs %>
